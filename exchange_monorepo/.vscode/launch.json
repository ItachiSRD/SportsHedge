{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Debug Nest Framework",
        "args": ["${workspaceFolder}/src/main.ts"],
        "runtimeArgs": ["--inspect-brk", "-r", "ts-node/register"],
        "cwd": "${workspaceFolder}",
        "protocol": "inspector",
        "internalConsoleOptions": "openOnSessionStart",
        "skipFiles": ["<node_internals>/**"]
      }
    ]
  }
  