{
  "name": "exchange-monorepo",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "export NODE_ENV=development && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "export NODE_ENV=production && nest build && node dist/main",
    "db:migrate:local": "export NODE_ENV=development && node src/core/database/migrations/main.js up",
    "db:migrate:local:down": "export NODE_ENV=development && node src/core/database/migrations/main.js down",
    "db:seed:local": "export NODE_ENV=development && node src/core/database/seeds/index.js up",
    "db:seed:local:down": "export NODE_ENV=development && node src/core/database/seeds/index.js down",
    "db:migrate:stage": "export NODE_ENV=stage && node src/core/database/migrations/main.js up",
    "db:migrate:stage:down": "export NODE_ENV=stage && node src/core/database/migrations/main.js down",
    "db:seed:stage": "export NODE_ENV=stage && node src/core/database/seeds/index.js up",
    "db:seed:stage:down": "export NODE_ENV=stage && node src/core/database/seeds/index.js down",
    "db:migrate:prod": "export NODE_ENV=prod && node src/core/database/migrations/main.js up",
    "db:migrate:prod:down": "export NODE_ENV=prod && node src/core/database/migrations/main.js down",
    "db:seed:prod": "export NODE_ENV=prod && node src/core/database/seeds/index.js up",
    "db:seed:prod:down": "export NODE_ENV=prod && node src/core/database/seeds/index.js down",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@aws-sdk/client-sqs": "^3.418.0",
    "@fastify/static": "^6.11.2",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-fastify": "^10.2.5",
    "@nestjs/swagger": "^7.1.11",
    "@types/bignumber.js": "^5.0.0",
    "axios": "^1.5.1",
    "big-number": "^2.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.3.1",
    "firebase-admin": "^11.11.0",
    "joi": "^17.10.2",
    "nanoid": "^3.3.6",
    "nestjs-pino": "^3.5.0",
    "pg": "^8.11.3",
    "pino-pretty": "^10.2.3",
    "postgres": "^3.3.5",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "sequelize": "^6.33.0",
    "sequelize-typescript": "^2.1.5",
    "sqs-consumer": "^7.3.0",
    "umzug": "^3.3.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/sequelize": "^4.28.15",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
